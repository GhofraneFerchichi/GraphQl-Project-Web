type Student {
  cin: String!
  nompre: String!
  email: String!
}

type Query {
  allStudents: [Student]
}
type Mutation {
  createStudent(cin: String!, nompre: String!, email: String!): Student
  updateStudent(cin:String, nompre:String, email:String): Boolean
  deleteStudent(cin:String):[Student]
}


schema {
  query: Query
  mutation: Mutation
}
